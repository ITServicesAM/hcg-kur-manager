<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="itservicesam.android.hcg_kur_manager.DetailsActivity"
    tools:showIn="@layout/activity_details">

    <TextView
        android:id="@+id/textViewDatum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:text="15.07.2016"
        app:layout_constraintLeft_toLeftOf="@+id/content_details"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/content_details"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gewicht"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintLeft_toLeftOf="@+id/textViewDatum"
        app:layout_constraintTop_toBottomOf="@+id/textViewDatum"
        android:layout_marginTop="16dp" />

    <EditText
        android:id="@+id/textViewGewicht"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right|center_vertical"
        android:hint="Gewicht eingeben"
        android:inputType="numberDecimal"
        android:maxLines="1"
        android:text="92"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintTop_toBottomOf="@+id/textViewDatum"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toLeftOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Körpermaße"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textViewGewicht"
        android:layout_marginTop="24dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bauch"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintLeft_toLeftOf="@+id/textView4"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewStomach" />

    <TextView
        android:id="@+id/textViewStandBauch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Stand 17.07.2016"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/textViewStomach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right|center_vertical"
        android:hint="Keine Angabe"
        android:inputType="number"
        android:maxLines="1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        tools:text="106"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        android:layout_marginTop="24dp"
        app:layout_constraintRight_toLeftOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rechter Oberschenkel"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewRightThigh" />

    <TextView
        android:id="@+id/textViewStandReOberSc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Stand 16.07.2016"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/textViewRightThigh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right|center_vertical"
        android:hint="Keine Angabe"
        android:inputType="number"
        android:maxLines="1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        tools:text="106"
        app:layout_constraintLeft_toLeftOf="@+id/textViewStomach"
        app:layout_constraintTop_toBottomOf="@+id/textViewStomach"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toLeftOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Linker Oberschenkel"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewLeftThigh" />

    <TextView
        android:id="@+id/textViewStandLiOberSc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Stand 15.07.2016"
        app:layout_constraintLeft_toLeftOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/textViewLeftThigh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:gravity="right|center_vertical"
        android:hint="Keine Angabe"
        android:inputType="number"
        android:maxLines="1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        tools:text="106"
        app:layout_constraintLeft_toLeftOf="@+id/textViewRightThigh"
        app:layout_constraintTop_toBottomOf="@+id/textViewRightThigh"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toLeftOf="@+id/textView19" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Brust"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintLeft_toLeftOf="@+id/textView6"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewChest" />

    <TextView
        android:id="@+id/textViewStandBrust"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Stand 15.07.2016"
        app:layout_constraintLeft_toLeftOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/textViewChest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right|center_vertical"
        android:hint="Keine Angabe"
        android:inputType="number"
        android:maxLines="1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        tools:text="106"
        app:layout_constraintLeft_toLeftOf="@+id/textViewLeftThigh"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/textViewLeftThigh"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Po"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintLeft_toLeftOf="@+id/textView7"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewButt" />

    <TextView
        android:id="@+id/textViewStandPo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Stand 15.07.2016"
        app:layout_constraintLeft_toLeftOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/textViewButt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:hint="Keine Angabe"
        android:inputType="number"
        android:maxLines="1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        tools:text="106"
        app:layout_constraintLeft_toLeftOf="@+id/textViewChest"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/textViewChest"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cm"
        app:layout_constraintRight_toRightOf="@+id/content_details"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewStomach" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="kg"
        app:layout_constraintRight_toRightOf="@+id/content_details"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewGewicht" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cm"
        app:layout_constraintRight_toRightOf="@+id/content_details"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewRightThigh" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cm"
        app:layout_constraintRight_toRightOf="@+id/content_details"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewLeftThigh" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cm"
        app:layout_constraintRight_toRightOf="@+id/content_details"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewChest" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cm"
        app:layout_constraintRight_toRightOf="@+id/content_details"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewButt" />



</android.support.constraint.ConstraintLayout>
